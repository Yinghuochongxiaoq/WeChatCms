{
  /* 上传图片配置项 */
  "imageFieldName": "upfile", /* 提交的图片表单名称 */
  "imageMaxSize": 41943040, /* 上传大小限制40M，单位B */
  "imageAllowFiles": [
    ".png",
    ".jpg",
    ".jpeg",
    ".gif",
    ".bmp",
    ".rar",
    ".zip",
    ".tar",
    ".gz",
    ".7z",
    ".bz2",
    ".cab",
    ".iso",
    ".doc",
    ".docx",
    ".xls",
    ".xlsx",
    ".ppt",
    ".pptx",
    ".pdf",
    ".txt",
    ".md",
    ".xml",
    ".mp4"
  ], /* 上传图片格式显示 */
  "imageUrlPrefix": "http://localhost:8004", /* 图片访问路径前缀 */
  "imagePathFormat": "/Uploadfile/{yyyy}{mm}{dd}/{time}{rand:6}" /* 上传保存路径,可以自定义保存路径和文件名格式 */
}
